<Page x:Class="Wow_Raid.HomePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Wow_Raid"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="525"
      Title="Home">

    <Grid Background="White">
        <Button x:Name="uploadLog" Content="Upload Log" HorizontalAlignment="Left" Margin="75,250,0,0" VerticalAlignment="Top" Width="75" Click="uploadLog_Click"/>
        <Button x:Name="viewLogs" Content="View Logs" HorizontalAlignment="Right" Margin="0,250,75,0" VerticalAlignment="Top" Width="75" Click="viewLog_Click"/>
        <Label x:Name="Title" Content="WoW Raid Investigator" HorizontalContentAlignment="Center" FontSize="32" HorizontalAlignment="Center" Height="92" Margin="58,109,59,0" VerticalAlignment="Top" Width="400"/>
    </Grid>
</Page>